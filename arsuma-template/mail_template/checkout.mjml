                        <td
                            style="
                                font-size: 18px;
                                padding-left: 10px;
                                color: #04194a;
                                font-family: Assistant, Helvetica, Arial,
                                    sans-serif;
                            "
                        >
                            support@arsuma.id<br />0857-555-22988
                        </td>
                    </tr>
                </mj-table>
            </mj-column>
        </mj-section>
        <mj-section padding-bottom="30px" padding-top="3px">
            <mj-column>
                <mj-text
                    align="center"
                    color="#A4A9B5"
                    font-size="18px"
                    font-family="Assistant, Helvetica, Arial,
                                    sans-serif"
                    padding-left="25px"
                    padding-right="25px"
                    padding-bottom="0px"
                    padding-top="30px"
                    >2021 Copyright Arsuma • All rights reserved • Made in
                    Malang
                </mj-text>
            </mj-column>
        </mj-section>
    </mj-body>
</mjml>
